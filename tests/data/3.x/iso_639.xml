<!DOCTYPE iso_639_entries [
	<!ELEMENT iso_639_entries (iso_639_entry+)>
	<!ELEMENT iso_639_entry EMPTY>
	<!ATTLIST iso_639_entry
		iso_639_2B_code		CDATA	#REQUIRED
		iso_639_2T_code		CDATA	#REQUIRED
		iso_639_1_code		CDATA	#IMPLIED
		name			CDATA	#REQUIRED
		common_name		CDATA	#IMPLIED
	>
]>

<iso_639_entries>
	<iso_639_entry
		iso_639_2B_code="aar"
		iso_639_2T_code="aar"
		iso_639_1_code="aa"
		name="Afar" />
	<iso_639_entry
		iso_639_2B_code="alg"
		iso_639_2T_code="alg"
		name="Algonquian languages" />
	<iso_639_entry
		iso_639_2B_code="heb"
		iso_639_2T_code="heb"
		iso_639_1_code="he"
		name="Hebrew" />
	<iso_639_entry
		iso_639_2B_code="tib"
		iso_639_2T_code="bod"
		iso_639_1_code="bo"
		name="Tibetan" />
</iso_639_entries>
